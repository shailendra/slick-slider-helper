{    
    "liveSassCompile.settings.formats":[
       {
           "format": "expanded",
           "extensionName": ".css",
           "savePath": "~/../assets/css/"
       },
       {
           "extensionName": "-min.css",
           "format": "compressed",
           "savePath": "~/../assets/css/"
       }
   ],
   "liveSassCompile.settings.autoprefix": [
        "> 1%",
        "last 20 versions",
        "firefox >=2",
        "ie >=8",
        "safari >=2",
        "chrome >=2",
        "opera >=2"
    ]
}